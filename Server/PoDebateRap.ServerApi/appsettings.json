{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.ApplicationInsights" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Azure": {
    "StorageConnectionString": "",
    "KeyVault": {
      "VaultUri": "https://kv-podebaterap.vault.azure.net/"
    },
    "OpenAI": {
      "Endpoint": "",
      "ApiKey": "",
      "DeploymentName": "gpt-4o"
    },
    "Speech": {
      "Region": "eastus",
      "Endpoint": "",
      "SubscriptionKey": ""
    },
    "ApplicationInsights": {
      "ConnectionString": ""
    }
  },
  "NewsApi": {
    "ApiKey": ""
  },
  "AllowedHosts": "*"
}
